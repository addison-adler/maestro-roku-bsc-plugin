{
  "stagingFolderPath": "/tmp/out",
  "rootDir": "src",
  "files": [
    "manifest",
    "source/**/*.*",
    "components/**/*.*",
    "images/**/*.*"
  ],
  "retainStagingFolder": true,
  "autoImportComponentScript": true,
  "diagnosticFilters": [
    1013,
    {
      "src": "components/roku_modules/mv/core/BaseView.brs",
      "codes": [
        1001
      ]
    }
  ],
  "diagnosticLevel": "warn",
  "buildTimeImports": {
    "IAuthProvider": [
      "pkg:/source/mixins/NetMixin.bs",
      "pkg:/source/maestro/observable/ObservableMixin.bs"
    ],
    "IEntitlementsProvider": [],
    "IBookmarksProvider": [],
    "IPlayerAnalytics": [],
    "IAnalytics": []
  },
  "plugins": [
    "../../dist/plugin.js"
  ]
}